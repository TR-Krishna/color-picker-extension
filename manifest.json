{
  "manifest_version": 3,
  "name": "Simple Color Picker",
  "version": "1.0",
  "description": "A basic color picker extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}